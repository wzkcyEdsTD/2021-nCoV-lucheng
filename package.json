{
  "name": "wzkcy-vue-cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "echarts": "^4.2.1",
    "echarts-wordcloud": "^1.1.3",
    "element-ui": "^2.13.0",
    "esri-loader": "^2.9.2",
    "jquery": "^3.4.1",
    "qs": "^6.8.0",
    "vue": "^2.6.10",
    "vue-calendar-component": "^2.8.2",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "node-sass": "^4.13.1",
    "px2rem-loader": "^0.1.9",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "vue-select": "^3.2.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-dupe-keys": "off",
      "vue/no-unused-vars": "off",
      "vue/no-unused-components": "off",
      "vue/return-in-computed-property": "off",
      "vue/no-side-effects-in-computed-properties": "off",
      "vue/no-use-v-if-with-v-for": "off",
      "no-extra-semi": "off",
      "no-redeclare": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
